%!PS-Adobe-3.0
%%EndComments

%%BeginProlog
%%BeginResource: TTTbook_init_page

/TTTbook_init_page
{
  << /PageSize [TTTbook_page_in_mm 72 mul 25.4 div dup] /MediaColor (white) >> setpagedevice
  72 25.4 div dup scale
  4 dict begin
    gsave
      TTTbook_page_in_mm 2 div TTTbook_page_in_mm 2 div translate
      45 rotate
      /HelveticaBold findfont 25 scalefont setfont
      0.95 setgray
      newpath 0 0 moveto (CHUPCKO) true charpath pathbbox
      /ury exch def
      /urx exch def
      /lly exch def
      /llx exch def
      newpath llx urx add 2 div neg lly ury add 2 div neg moveto (CHUPCKO) show
    grestore
  end
  gsave
    TTTbook_footer_font_name findfont TTTbook_footer_font_size scalefont setfont
    TTTbook_footer_gray setgray
    newpath 7.5 7.5 moveto TTTbook_footer_text show
  grestore
} def

%%EndResource
%%BeginResource: TTTbook_helper

/TTTbook_helper
{
  0.01 setlinewidth
  0 0 0.75 setrgbcolor
  0 2.5 TTTbook_page_in_mm
  {
    /i exch def
    newpath i 0 moveto i TTTbook_page_in_mm lineto stroke
    newpath 0 i moveto TTTbook_page_in_mm i lineto stroke
  } for
} def

%%EndResource
%%BeginResource: TTTbook_page_number

/TTTbook_page_number
{
  5 dict begin
    /page_number exch def
    gsave
      TTTbook_page_number_font_name findfont TTTbook_page_number_font_size scalefont setfont
      TTTbook_page_number_gray setgray
      newpath 0 0 moveto page_number true charpath pathbbox
      /ury exch def
      /urx exch def
      /lly exch def
      /llx exch def
      newpath 112.5 urx sub 7.5 moveto page_number show
    grestore
  end
} def

%%EndResource
%%BeginResource: TTTbook_status

/TTTbook_status
{
  5 dict begin
    /status exch def
    gsave
      TTTbook_status_font_name findfont TTTbook_status_font_size scalefont setfont
      TTTbook_status_gray setgray
      newpath 0 0 moveto status true charpath pathbbox
      /ury exch def
      /urx exch def
      /lly exch def
      /llx exch def
      newpath 7.5 llx sub 112.5 ury sub moveto status show
    grestore
  end
} def

%%EndResource
%%BeginResource: TTTbook_hash

/TTTbook_hash
{
  gsave
    TTTbook_hash_line_width setlinewidth
    TTTbook_hash_gray setgray
    newpath 47.5 22.5 moveto 47.5 97.5 lineto stroke
    newpath 72.5 22.5 moveto 72.5 97.5 lineto stroke
    newpath 22.5 47.4 moveto 97.5 47.5 lineto stroke
    newpath 22.5 72.5 moveto 97.5 72.5 lineto stroke
  grestore
} def

%%EndResource
%%BeginResource: TTTbook_find

/TTTbook_find
{
  2 dict begin
    /y exch def
    /x exch def
    35 x 25 mul add
    85 y 25 mul sub
  end
} def

%%EndResource
%%BeginResource: TTTbook_empty

/TTTbook_empty
{
  pop
  pop
} def

%%EndResource
%%BeginResource: TTTbook_x

/TTTbook_x
{
  gsave
    TTTbook_find translate
    TTTbook_x_line_width setlinewidth
    TTTbook_x_gray setgray
    newpath -7.5 -7.5 moveto 7.5 7.5 lineto stroke
    newpath -7.5 7.5 moveto 7.5 -7.5 lineto stroke
  grestore
} def

%%EndResource
%%BeginResource: TTTbook_x_gray

/TTTbook_x_last
{
  gsave
    TTTbook_find translate
    TTTbook_x_line_width setlinewidth
    TTTbook_x_last_gray setgray
    newpath -7.5 -7.5 moveto 7.5 7.5 lineto stroke
    newpath -7.5 7.5 moveto 7.5 -7.5 lineto stroke
  grestore
} def

%%EndResource
%%BeginResource: TTTbook_o

/TTTbook_o
{
  gsave
    TTTbook_find translate
    TTTbook_o_line_width setlinewidth
    TTTbook_o_gray setgray
    newpath 0 0 7.5 0 360 arc stroke
  grestore
} def

%%EndResource
%%BeginResource: TTTbook_o_gray

/TTTbook_o_last
{
  gsave
    TTTbook_find translate
    TTTbook_o_line_width setlinewidth
    TTTbook_o_last_gray setgray
    newpath 0 0 7.5 0 360 arc stroke
  grestore
} def

%%EndResource
%%BeginResource: TTTbook_go_to

/TTTbook_go_to
{
  1 dict begin
    /go_to exch def
    gsave
      TTTbook_find translate
      TTTbook_go_to_font_name findfont TTTbook_go_to_font_size scalefont setfont
      TTTbook_go_to_gray setgray
      newpath 0 0 moveto go_to true charpath pathbbox
      /ury exch def
      /urx exch def
      /lly exch def
      /llx exch def
      newpath llx urx add 2 div neg lly ury add 2 div neg moveto go_to show
    grestore
  end
} def

%%EndResource
%%EndProlog

%%BeginSetup

/TTTbook_page_in_mm 120 def

/TTTbook_footer_text (TTTbook $$VERSION $$DATE by CHUPCKO) def
/TTTbook_footer_font_name /Helvetica def
/TTTbook_footer_font_size 2 def
/TTTbook_footer_gray 0.5 def
/TTTbook_page_number_font_name /HelveticaBold def
/TTTbook_page_number_font_size 5 def
/TTTbook_page_number_gray 0 def
/TTTbook_status_font_name /HelveticaBold def
/TTTbook_status_font_size 5 def
/TTTbook_status_gray 0 def
/TTTbook_hash_line_width 2 def
/TTTbook_hash_gray 0 def
/TTTbook_x_line_width 2 def
/TTTbook_x_gray 0 def
/TTTbook_x_last_gray 0.5 def
/TTTbook_o_line_width 2 def
/TTTbook_o_gray 0 def
/TTTbook_o_last_gray 0.5 def
/TTTbook_go_to_font_name /HelveticaBold def
/TTTbook_go_to_font_size 5 def
/TTTbook_go_to_gray 0 def

%%EndSetup

%%Page: 3 5
TTTbook_init_page
(PPP) TTTbook_status
(1) TTTbook_page_number
TTTbook_hash
0 0 TTTbook_x
1 2 TTTbook_x
2 2 TTTbook_o_last
1 1 (23) TTTbook_go_to
TTTbook_helper
showpage

%%Page: 2 7
TTTbook_init_page
(Book win!) TTTbook_status
(343) TTTbook_page_number
TTTbook_hash
0 0 TTTbook_x_last
1 2 TTTbook_o
1 1 (983) TTTbook_go_to
1 1 (983) TTTbook_go_to
showpage

%%EOF
